                "mq": {
                    "bind_interface": "eth1",
                    "host": "10.0.3.11",
                    "user": "guest",
                    "vhost": "/",
                    "network":{
                        "rabbit":{
                            "host": "10.0.3.11",
                            "port": "5672"
                        }
                    },
                    "compute":{
                        "service_type": "rabbitmq",
                        "rabbit":{
                            "host": "10.0.3.11",
                            "port": "5672"
                        }
                    },
                    "block-storage":{
                        "service_type": "rabbitmq",
                        "rabbit":{
                            "host": "10.0.3.11",
                            "port": "5672"
                        }
                    }
                },
                "db": {
                    "bind_interface": "eth1",
                    "host": "10.0.3.11"
                }
            },
            "network": {
                "debug": "True",
                "service_plugins": [ "router" ],
                "dhcp": {
                    "enable_isolated_metadata": "True"
                },
                "metadata": {
                  "nova_metadata_ip": "10.0.3.11"
                },
                "allow_overlapping_ips": "True",
                "verbose": "True",
                "linux_bridge": {
                  "firewall_driver": "neutron.agent.linux.iptables_firewall.OVSHybridIptablesFirewallDriver"
                },
                "ml2": {
                   "type_drivers": "flat,gre",
                   "tenant_network_type": "gre",
                   "mechanism_drivers": "openvswitch",
                   "flat_networks": "external",
                   "tunnel_id_ranges": "1:100",
                   "enable_security_group": "True",
                   "tenant_network_types": "gre",
                   "enable_ipset": "True"
                },
                "openvswitch": {
                    "type_drivers": "flat,gre",
                    "mechanism_drivers": "openvswitch",
                    "tunnel_id_ranges": "1:1000",
                    "enable_tunneling": "True",
                    "tunnel_type": "gre",
                    "tunnel_types": "gre",
                    "tenant_network_type": "gre",
                    "local_ip_interface": "eth2",
                    "bridge_mappings": "external:br-ex"
                },
                "api": {
                    "bind_interface": "eth1"
                },
                "quota": {
                    "floatingip": "50"
                },
                "l3":{
                    "external_network_bridge_interface": "eth3"
                }
            },
            "block-storage": {
                "volume": {
                    "create_volume_group": true,
                    "default_volume_type": "lvm"
                }
            },
            "image": {
                "api": {
                    "bind_interface": "eth1"
                },
                "debuge": true,
                "identity_service_chef_role": "os-identity",
                "rabbit_server_chef_role": "os-ops-messaging",
                "registry": {
                    "bind_interface": "eth1"
                },
                "image_upload": true,
                "upload_images": [
                    "cirros"
                ],
                "upload_image": {
                    "cirros": "https://launchpad.net/cirros/trunk/0.3.0/+download/cirros-0.3.0-x86_64-disk.img"
                }
            },
            "compute": {
                "rabbit":{
                  "host": "10.0.3.11"
                },
                "enabled_apis": "ec2,osapi_compute",
                "novnc_proxy": {
                    "bind_interface": "eth1",
                    "bind_address": "0.0.0.0"
                },
                "xvpvnc_proxy": {
                    "bind_interface": "eth1",
                    "bind_address": "0.0.0.0"
                },
                "libvirt": {
                    "virt_type": "qemu"
                },
                "network": {
                    "public_interface": "eth1",
                    "service_type": "neutron",
                    "neutron": {
                      "libvirt_vif_driver": "nova.virt.libvirt.vif.LibvirtHybridOVSBridgeDriver"
                    }
                },
                "config": {
                    "ram_allocation_ratio": 5.0
                }
            },
            "orchestration": {
                "api":{
                  "bind_interface": "eth1"
                },
                "api-cfn": {
                  "bind_interface": "eth1"
                },
                "api-cloudwatch":{
                  "bind_interface": "eth1"
                }
            }
